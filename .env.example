# Supabase Configuration
# Get these from: https://app.supabase.com/project/YOUR_PROJECT/settings/api
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# N8N Configuration
# Get webhook URLs from N8N Cloud after creating workflows
N8N_WEBHOOK_SHOPIFY=https://your-instance.app.n8n.cloud/webhook/shopify
N8N_WEBHOOK_KLAVIYO=https://your-instance.app.n8n.cloud/webhook/klaviyo

# External APIs
OPENAI_API_KEY=sk-...
SHOPIFY_API_KEY=your-shopify-key
SHOPIFY_API_SECRET=your-shopify-secret
SHOPIFY_ACCESS_TOKEN=your-access-token
KLAVIYO_API_KEY=your-klaviyo-private-key

# App Configuration
DEFAULT_WORKSPACE_ID=00000000-0000-0000-0000-000000000000
NODE_ENV=development